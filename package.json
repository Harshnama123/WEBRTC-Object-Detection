{
  "name": "webrtc-object-detection",
  "version": "1.0.0",
  "description": "Real-time object detection via WebRTC",
  "main": "backend/server.js",
  "scripts": {
    "start": "node backend/server.js",
    "dev": "nodemon backend/server.js",
    "setup": "npm install && npm install socket.io-client",
    "benchmark": "node benchmark.js --duration 30 --mode wasm",
    "benchmark-wasm": "node benchmark.js --duration 30 --mode wasm",
    "benchmark-server": "node benchmark.js --duration 30 --mode server",
    "docker-build": "docker-compose build",
    "docker-up": "docker-compose up --build",
    "test": "node benchmark.js --duration 5 --mode wasm"
  },
  "dependencies": {
    "express": "^4.18.2",
    "socket.io": "^4.7.2",
    "socket.io-client": "^4.8.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  },
  "keywords": [
    "webrtc",
    "object-detection",
    "tensorflow",
    "ai",
    "real-time"
  ],
  "author": "Your Name",
  "license": "MIT"
}
